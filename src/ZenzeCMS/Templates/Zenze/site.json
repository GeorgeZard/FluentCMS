{
    "Name": "Zenze.FluentCMS",
    "Description": "Zenze.FluentCMS default site template including some samples",
    "Layout": "DefaultLayout",
    "EditLayout": "EditLayout",
    "DetailLayout": "SingleLayout",
    "AdminRoles": [ "Administrators" ],
    "ContributorRoles": [ "Contributors" ],
    "Settings": {
        "MetaTitle": "Zenze.CMS",
        "MetaDescription": "Zenze.CMS is an Blazor CMS designed to streamline content management",
        "GoogleTagsId": "",
        "Head": "",
        "Index": "true",
        "Follow": "true",
        "OgType": "website",
        "RobotsTxt": "User-agent: *\nDisallow: /admin"
