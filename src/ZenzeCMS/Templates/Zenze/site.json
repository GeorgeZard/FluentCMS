    "ContentTypes": [
        {
            "Slug": "blog",
            "Title": "Blog Post",
            "Description": "Blog post content type",
            "Fields": [
                {
                    "Name": "Title",
                    "Description": "Blog post title",
                    "Type": "string",
                    "Required": true,
                    "Label": "Title",
                    "Settings": {
                        "Placeholder": "Enter post title",
                        "DataTableColumnOrder": 1,
                        "DataTableVisible": true,
                        "DataTableViewComponent": "StringFieldDataTableView",
                        "FormViewOrder": 1,
                        "FormColWidth": 6,
                        "FormViewComponent": "StringFieldFormText"
                    }
                },
                {
                    "Name": "Content",
                    "Type": "string",
                    "Required": true,
                    "Description": "Blog post content",
                    "Label": "Content",
                    "Settings": {
                        "Placeholder": "Enter post content",
                        "DataTableVisible": false,
                        "DataTableViewComponent": "StringFieldDataTableView",
                        "FormViewOrder": 4,
                        "FormColWidth": 12,
                        "FormViewComponent": "StringFieldFormRichText"
                    }
                },
                {
                    "Name": "FeaturedImage",
                    "Type": "single-file",
                    "Label": "Featured Image",
                    "Description": "Blog post main image",
                    "Required": false,
                    "Settings": {
                        "Placeholder": "Choose post main image",
                        "DataTableVisible": true,
                        "DataTableViewComponent": "SingleFileFieldDataTableFile",
                        "FormViewOrder": 4,
                        "FormColWidth": 12,
                        "FormViewComponent": "SingleFileFieldFormFilePicker"
                    }
                },
                {
                    "Name": "Slug",
                    "Type": "string",
                    "Required": true,
                    "Unique": true,
                    "Description": "Url slug of the post",
                    "Label": "Slug",
                    "Settings": {
                        "Placeholder": "Enter url slug of the post",
                        "DataTableColumnOrder": 2,
                        "DataTableVisible": true,
                        "DataTableViewComponent": "StringFieldDataTableView",
                        "FormViewOrder": 2,
                        "FormColWidth": 6,
                        "FormViewComponent": "StringFieldFormText",
                        "Pattern": "[a-zA-Z0-9-]"
                    }
                },
                {
                    "Name": "Order",
                    "Type": "number",
                    "Required": true,
                    "Description": "Blog post view order",
                    "Label": "View Order",
                    "Settings": {
                        "Placeholder": "Enter view order of the post",
                        "DataTableColumnOrder": 2,
                        "DataTableVisible": true,
                        "DataTableViewComponent": "NumberFieldDataTableView",
                        "FormViewOrder": 8,
                        "FormColWidth": 6,
                        "FormViewComponent": "NumberFieldFormInput"
                    }
                },
                {
                    "Name": "Status",
                    "Type": "single-select",
                    "Required": true,
                    "Description": "status of the blog post",
                    "Label": "Status",
                    "Settings": {
                        "Placeholder": "Choose status of post",
                        "DataTableColumnOrder": 4,
                        "DataTableVisible": true,
                        "DataTableViewComponent": "SelectFieldDataTableBadge",
                        "FormViewOrder": 5,
                        "FormColWidth": 12,
