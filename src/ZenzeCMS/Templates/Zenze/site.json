    "Pages": [
        {
            "Title": "Home",
            "Path": "/",
            "EditLayout": "EditLayout",
            "ViewRoles": [ "All Users" ],
            "Settings": {
                "Index": "true",
                "Follow": "true"
            },
            "Plugins": [
                {
                    "Definition": "TextHTML Plugin",
                    "Section": "Main",
                    "Type": "TextHTMLContent",
                    "Content": [
                        {
                            "Content": "<div class=\"max-w-screen-xl px-4 py-8 mx-auto lg:py-16\">\n    <div class=\"grid items-center gap-8 mb-8 lg:mb-16 lg:gap-12 lg:grid-cols-12\">\n        <div class=\"col-span-12 sm:mb-6 text-center lg:mb-0\">\n            <a href=\"https://github.com/fluentcms/FluentCMS\"\n                class=\"inline-flex items-center justify-between px-1 py-1 pr-4 mb-6 text-sm text-gray-700 bg-gray-50 rounded-full dark:bg-gray-800 dark:text-white hover:bg-gray-200 dark:hover:bg-gray-700\"\n                role=\"alert\">\n                <span class=\"px-3 py-1 mr-3 text-xs text-white rounded-full bg-blue-600\" data-inline-editable>New</span> <span\n                    class=\"text-sm font-medium\" data-inline-editable>See our latest progress</span>\n                <svg class=\"w-5 h-5 ml-2\" fill=\"currentColor\" viewBox=\"0 0 20 20\" xmlns=\"http://www.w3.org/2000/svg\">\n                    <path fill-rule=\"evenodd\"\n                        d=\"M7.293 14.707a1 1 0 010-1.414L10.586 10 7.293 6.707a1 1 0 011.414-1.414l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414 0z\"\n                        clip-rule=\"evenodd\"></path>\n                </svg>\n            </a>\n            <h1\n                class=\"text-center my-8 text-4xl font-extrabold leading-none tracking-tight text-gray-900 md:text-3xl xl:text-4xl dark:text-white\" data-inline-editable>\n                Welcome to FluentCMS\n                <div class=\"mt-6\" data-inline-editable>\n                    AI Driven ASP.NET Core Blazor Content Management System (CMS)\n                </div>\n            </h1>\n            <p\n                class=\"text-center mx-auto max-w-xl mx-auto mb-6 font-light text-gray-500 xl:mb-8 md:text-lg xl:text-xl dark:text-gray-400\" data-inline-editable>\n                Building and managing content has never been easier. Experience the power of AI with FluentCMS.\n            </p>\n            <div class=\"flex justify-center gap-4\">\n                <img src=\"https://img.shields.io/github/issues/fluentcms/FluentCMS\" alt=\"Issue count\">\n                <img class=\"hidden\" src=\"https://img.shields.io/nuget/v/FluentCMS\" alt=\"Version\">\n                <img src=\"https://img.shields.io/badge/version-1.2.3-blue\" alt=\"Version\">\n            </div>\n            <div class=\"pt-12\">\n                <a href=\"/admin\"\n                    class=\"text-white bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:ring-blue-300 font-medium rounded-lg text-sm px-4 lg:px-5 py-2 lg:py-2.5 mr-2 dark:bg-blue-600 dark:hover:bg-blue-700 focus:outline-none dark:focus:ring-blue-800\" data-inline-editable>\n                    Open Administration\n                </a>\n            </div>\n        </div>\n    </div>\n</div>"
                        }
                    ]
                }
            ]
        },
        {
            "Title": "Blogs",
            "Path": "/blogs",
            "Layout": "SingleLayout",
            "EditLayout": "EditLayout",
            "ViewRoles": [ "All Users" ],
            "Settings": {
                "Index": "true",
                "Follow": "true"
            },
            "Plugins": [

                {
                    "Definition": "TextHTML Plugin",
                    "Section": "Main",
                    "Type": "TextHTMLContent",
                    "Content": [
                        {
                            "Content": "<section><h1>Blog</h1></section>"
                        }
                    ]
                },
                {
                    "Definition": "Content List",
                    "Section": "Main",
                    "Cols": 12,
                    "Settings": {
                        "Template": "<section class=\"blog_section section_space_lg\"> <div class=\"container\"> <div class=\"blog_grouped row\"> {{ for item in Items }} <div class=\"col col-lg-4 col-md-6\"> <div class=\"blog_item\"> <a class=\"item_image\" href=\"/blogs/detail/{{ item.Id }}\" data-cursor-text=\"VIEW\"> {{ if item.FeaturedImage }} <img src=\"{{ item.FeaturedImage }}\" alt=\"{{ item.Title }}\"> {{ else }} <img src=\"/files/images/blogs/blog_image_1.jpg\" alt=\"{{ item.Title }}\"> {{ end }} </a> <div class=\"item_content\"> <div class=\"flex_wrap\"> <ul class=\"category_list unordered_list\"> {{ for tag in item.Tags }} <li><a href=\"#!\">{{ tag }}</a></li> {{ end }} </ul> <span class=\"post_date\">{{ item.PublishedAt }}</span> </div> <h3 class=\"item_title\"> <a href=\"/blogs/detail/{{ item.Id }}\"> {{ item.Title }} </a> </h3> <a class=\"btn-link\" href=\"/blogs/detail/{{ item.Id }}\"> <span class=\"btn_text\">Read More</span> <span class=\"btn_icon\"> <img src=\"/files/images/icons/icon_arrow_down_right.svg\" alt=\"Paradox - Icon Arrow Down Right\"> <img src=\"/files/images/icons/icon_arrow_down_right_primary.svg\" alt=\"Paradox - Icon Arrow Down Right\"> </span> </a> </div> </div> </div> {{ end }} </div> </div>",
                        "ContentTypeSlug": "blog"
                    }
                }
            ],
            "Children": [
                {
                    "Title": "Blog Detail",
                    "Path": "/detail",
                    "Layout": "SingleLayout",
                    "ViewRoles": [ "All Users" ],
                    "Settings": {
                        "Index": "true",
                        "Follow": "true"
                    },
                    "Plugins": [
                        {
                            "Definition": "TextHTML Plugin",
                            "Section": "Main",
                            "Cols": 12,
                            "Content": [
                                {
                                    "Content": "<section><h1>Blog Detail</h1></section>"
                                }
                            ]
                        },
                        {
                            "Definition": "Content Detail",
                            "Section": "Main",
                            "Cols": 12,
                            "Settings": {
                                "Template": "<section class=\"details_section blog_details section_space_lg\">\n  <div class=\"container\">\n    <div class=\"row justify-content-center\">\n      <div class=\"col col-lg-12\">\n        <div class=\"details_image\">\n          <img src=\"{{ Item.FeaturedImage }}\" alt=\"{{ Item.Title }}\">\n        </div>\n\n        <div class=\"details_content\">\n          <div class=\"blog_post_meta\">\n            <ul class=\"category_list unordered_list\">\n              <li><a href=\"#!\">UI/UX</a></li>\n            </ul>\n            <span class=\"post_date\">{{ Item.PublishedAt }}</span>\n          </div>\n          <h2 class=\"details_item_title\">\n            {{ Item.Title }}\n          </h2>\n          <p>\n           {{ Item.Content }}\n          </p>\n        \n          <div class=\"social_actions\">\n            <ul class=\"tags_list unordered_list\">\n {{ for tag in Item.Tags }}\n                 <li>{{tag}}</li>\n                </span>\n            {{ end }}\n            </ul>\n            \n          </div>\n        </div>\n\n        \n      </div>\n    </div>\n  </div>\n</section>",
                                "ContentTypeSlug": "blog"
                            }
                        }
                    ]
                }
            ]
        },
